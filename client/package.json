{
    "name": "ai-dialer-frontend",
    "version": "1.0.0",
    "description": "React frontend for AI Dialer Pro",
    "private": true,
    "dependencies": {
        "@heroicons/react": "^2.2.0",
        "@tanstack/react-query": "^5.90.2",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.5.1",
        "autoprefixer": "^10.4.16",
        "axios": "^1.12.2",
        "clsx": "^2.0.0",
        "jssip": "^3.10.0",
        "lucide-react": "^0.294.0",
        "postcss": "^8.4.32",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.48.2",
        "react-hot-toast": "^2.6.0",
        "react-router-dom": "^6.20.1",
        "react-scripts": "^5.0.1",
        "recharts": "^2.8.0",
        "socket.io-client": "^4.7.4",
        "tailwindcss": "^3.3.6",
        "ws": "^8.14.2"
    },
    "scripts": {
        "start": "set GENERATE_SOURCEMAP=false && set PORT=3001 && react-scripts start",
        "start:dev": "set GENERATE_SOURCEMAP=false && set HOST=localhost && set PORT=3000 && react-scripts start",
        "start:prod": "set GENERATE_SOURCEMAP=false && set HOST=0.0.0.0 && set PORT=3000 && react-scripts start",
        "start:clean": "npm run clean && npm start",
        "build": "set GENERATE_SOURCEMAP=false && react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "clean": "rm -rf node_modules/.cache && rm -rf build",
        "lint": "eslint src/**/*.{js,jsx}",
        "lint:fix": "eslint src/**/*.{js,jsx} --fix",
        "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
        "lint:check": "eslint src/**/*.{js,jsx} --format=compact",
        "fix-all": "npm run lint:fix && npm run format"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.28.4",
        "@eslint/js": "^9.37.0",
        "@types/react": "^18.2.42",
        "@types/react-dom": "^18.2.17",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^6.1.1",
        "globals": "^16.4.0",
        "prettier": "^3.6.2"
    }
}